<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>hunqin</display-name>
  <servlet>
    <servlet-name>dengru</servlet-name>
    <servlet-class>com.hunqin.demo.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>dengru</servlet-name>
    <url-pattern>/login/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>zhuce</servlet-name>
    <servlet-class>com.hunqin.demo.zhuce</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>zhuce</servlet-name>
    <url-pattern>/zhuce/zhuce</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <servlet-name>yzm</servlet-name>
    <servlet-class>com.hunqin.demo.yzm</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>yzm</servlet-name>
    <url-pattern>/yzm</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>modify</display-name>
    <servlet-name>modify</servlet-name>
    <servlet-class>com.hunqin.demo.modify</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>modify</servlet-name>
    <url-pattern>/modify</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PayServlet</servlet-name>
    <servlet-class>com.hunqin.Pay.PayServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BackServlet</servlet-name>
    <servlet-class>com.hunqin.Pay.BackServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PayServlet</servlet-name>
    <url-pattern>/payServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BackServlet</servlet-name>
    <url-pattern>/backServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CartControl</display-name>
    <servlet-name>CartControl</servlet-name>
    <servlet-class>com.hunqin.shooping.CartControl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CartControl</servlet-name>
    <url-pattern>/CartControl</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <servlet-name>DetailServlet</servlet-name>
    <servlet-class>com.hunqin.servlet.CartServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DetailServlet</servlet-name>
    <url-pattern>/cart</url-pattern>
  </servlet-mapping>
 

</web-app>
